<h1>All Events</h1>
<ul><% @events.each do |event| %>
  <% event.creator.ranks.each do |a_rank| %>
    <% if a_rank.rankee.id == 1 && a_rank.rank > 1 %>
      <li><%= link_to event.name, event %>
        <% if User.find(1).events.include? event %>
          <%= button_to 'Cancel', {:controller => "events", :action => "unattend", :params => {:attendee => {:event_id => event.id, :user_id=> 1, hidden: true}}} , :method=>:update  %>
        <% else %>
           <%= button_to 'Attend', {:controller => "events", :action => "attend", :params => {:attendee => {:event_id => event.id, :user_id=> 1}}} , :method=>:post  %>
        <%# :user_id=> session[:user_id]}  %>
        <% end %>
      </li>
    <% end %>
  <% end %>
<% end %></ul>
